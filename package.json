{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "author": "that70schris",
  "commands": [
    {
      "description": "Show weather in menu bar",
      "interval": "1m",
      "mode": "menu-bar",
      "name": "menubar",
      "preferences": [
        {
          "default": true,
          "description": "Show Icon",
          "label": "Show Icon",
          "name": "showIcon",
          "required": false,
          "title": "Appearance",
          "type": "checkbox"
        },
        {
          "default": true,
          "description": "Show Text",
          "label": "Show Text",
          "name": "showText",
          "required": false,
          "type": "checkbox"
        }
      ],
      "title": "Weather Menu Bar"
    }
  ],
  "dependencies": {
    "@raycast/api": "1.75.1",
    "@raycast/utils": "1.15.0",
    "applescript": "1.0.0",
    "cross-fetch": "4.0.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21"
  },
  "description": "Weather forecast from Weather App",
  "devDependencies": {
    "@raycast/eslint-config": "1.0.8",
    "@types/lodash": "4.17.4",
    "@types/node": "20.12.12",
    "@types/react": "18.3.3",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "eslint": "9.3.0",
    "prettier": "3.2.5",
    "react": "18.3.1",
    "typescript": "5.4.5"
  },
  "icon": "Icon.png",
  "license": "MIT",
  "name": "weather",
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  },
  "title": "Weather",
  "version": "0.0.0"
}
